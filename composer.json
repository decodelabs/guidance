    "type": "library",
    "keywords": [ ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/exceptional": "^0.6.3",
        "decodelabs/kingdom": "^0.1",
        "decodelabs/nuance": "^0.1.9",

        "brick/math": "^0.12|^0.13",
        "ramsey/uuid": "^4.8.1"
    },
    "require-dev": {
